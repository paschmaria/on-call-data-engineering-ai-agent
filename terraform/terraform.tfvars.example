# Example Terraform variables file
# Copy this to terraform.tfvars and fill in your values

# AWS Configuration
aws_region = "us-east-1"

# Environment
environment = "dev"  # Options: dev, test, prod

# MWAA Configuration
mwaa_environment_name = "your-mwaa-environment-name"

# Redshift Configuration
redshift_cluster_id = "your-redshift-cluster-id"

# Slack Configuration
slack_channel_id = "C1234567890"  # Your Slack channel ID

# Lambda Configuration (optional - has defaults)
# lambda_timeout = 300
# lambda_memory = 1024
# log_retention_days = 30